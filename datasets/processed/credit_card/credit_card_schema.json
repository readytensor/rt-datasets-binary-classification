{
  "title": "German credit dataset",
  "description": "This dataset classifies people described by a set of attributes as good or bad credit risks.",
  "modelCategory": "binary_classification",
  "schemaVersion": 1.0,
  "inputDataFormat": "CSV",
  "id": {
    "name": "id",
    "description": "unique record identifier"
  },
  "target": {
    "name": "class",
    "description": "good or bad credit",
    "classes": [
      "good",
      "bad"
    ]
  },
  "features": [
    {
      "name": "checking_status",
      "description": "Status of existing checking account",
      "dataType": "CATEGORICAL",
      "categories": [
        "0<=X<200",
        "<0",
        ">=200",
        "no checking"
      ],
      "nullable": false
    },
    {
      "name": "duration",
      "description": "Duration in month",
      "dataType": "NUMERIC",
      "example": 6,
      "nullable": false
    },
    {
      "name": "credit_history",
      "description": "Credit history",
      "dataType": "CATEGORICAL",
      "categories": [
        "all paid",
        "critical/other existing credit",
        "delayed previously",
        "existing paid",
        "no credits/all paid"
      ],
      "nullable": false
    },
    {
      "name": "purpose",
      "description": "Purpose",
      "dataType": "CATEGORICAL",
      "categories": [
        "business",
        "domestic appliance",
        "education",
        "furniture/equipment",
        "new car",
        "other",
        "radio/tv",
        "repairs",
        "retraining",
        "used car"
      ],
      "nullable": false
    },
    {
      "name": "credit_amount",
      "description": "Credit amount",
      "dataType": "NUMERIC",
      "example": 1169,
      "nullable": false
    },
    {
      "name": "savings_status",
      "description": "Savings account/bonds",
      "dataType": "CATEGORICAL",
      "categories": [
        "100<=X<500",
        "500<=X<1000",
        "<100",
        ">=1000",
        "no known savings"
      ],
      "nullable": false
    },
    {
      "name": "employment",
      "description": "Present employment since",
      "dataType": "CATEGORICAL",
      "categories": [
        "1<=X<4",
        "4<=X<7",
        "<1",
        ">=7",
        "unemployed"
      ],
      "nullable": false
    },
    {
      "name": "installment_rate",
      "description": "Installment rate in percentage of disposable income",
      "dataType": "NUMERIC",
      "example": 4,
      "nullable": false
    },
    {
      "name": "personal_status",
      "description": "Personal status and sex",
      "dataType": "CATEGORICAL",
      "categories": [
        "female div/dep/mar",
        "male div/sep",
        "male mar/wid",
        "male single"
      ],
      "nullable": false
    },
    {
      "name": "other_parties",
      "description": "Other debtors / guarantors",
      "dataType": "CATEGORICAL",
      "categories": [
        "co applicant",
        "guarantor",
        "none"
      ],
      "nullable": false
    },
    {
      "name": "residence_since",
      "description": "Present residence since",
      "dataType": "NUMERIC",
      "example": 4,
      "nullable": false
    },
    {
      "name": "property",
      "description": "Property",
      "dataType": "CATEGORICAL",
      "categories": [
        "car",
        "life insurance",
        "no known property",
        "real estate"
      ],
      "nullable": false
    },
    {
      "name": "age",
      "description": "Age in years",
      "dataType": "NUMERIC",
      "example": 67,
      "nullable": false
    },
    {
      "name": "other_payment_plans",
      "description": "Other installment plans ",
      "dataType": "CATEGORICAL",
      "categories": [
        "bank",
        "none",
        "stores"
      ],
      "nullable": false
    },
    {
      "name": "housing",
      "description": "Housing",
      "dataType": "CATEGORICAL",
      "categories": [
        "for free",
        "own",
        "rent"
      ],
      "nullable": false
    },
    {
      "name": "existing_credits",
      "description": "Number of existing credits at this bank",
      "dataType": "NUMERIC",
      "example": 2,
      "nullable": false
    },
    {
      "name": "job",
      "description": "Job",
      "dataType": "CATEGORICAL",
      "categories": [
        "high qualif/self emp/mgmt",
        "skilled",
        "unemp/unskilled non res",
        "unskilled resident"
      ],
      "nullable": false
    },
    {
      "name": "num_dependents",
      "description": "Number of people being liable to provide maintenance for",
      "dataType": "NUMERIC",
      "example": 1,
      "nullable": false
    },
    {
      "name": "own_telephone",
      "description": "Telephone",
      "dataType": "CATEGORICAL",
      "categories": [
        "none",
        "yes"
      ],
      "nullable": false
    },
    {
      "name": "foreign_worker",
      "description": "Foreign worker",
      "dataType": "CATEGORICAL",
      "categories": [
        "no",
        "yes"
      ],
      "nullable": false
    }
  ]
}